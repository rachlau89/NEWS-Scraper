{
    "name": "discord-bot-template",
    "version": "2.3.0",
    "type": "module",
    "description": "An open-source discord bot template",
    "scripts": {
        "app": "node src/index.js",
        "start": "npm run lint && npm run app",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "eslintConfig": {
        "extends": "@hazmi35/eslint-config/node",
        "parserOptions": {
            "sourceType": "module"
        }
    },
    "engines": {
        "node": ">=16.6.x",
        "npm": ">=7.x.x"
    },
    "author": "Rajat Mishra",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.1.3",
        "cheerio": "^1.0.0-rc.12",
        "discord.js": "^13.5.0",
        "dotenv": "^10.0.0",
        "json-loader": "^0.5.7",
        "simple-json-db": "^2.0.0"
    },
    "devDependencies": {
        "@hazmi35/eslint-config": "^7.0.1",
        "eslint": "^8.2.0",
        "eslint-plugin-import": "^2.25.2"
    }
